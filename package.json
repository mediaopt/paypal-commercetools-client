  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@paypal/react-paypal-js": "^8.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "classnames": "^2.3.2",
    "i18next": "^23.6.0",
    "libphonenumber-js": "^1.10.49",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^13.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "clean": "rimraf dist",
    "copy-files": "copyfiles -e *.ts -e **/*.ts -e **/*.tsx -u 1 src/**/**/*.css dist/esm/src",
    "tsc-local": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run copy-files",
    "prepare-local": "npm run tsc",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run copy-files && npm run build",
    "prepare": "npm run tsc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "tailwindcss": "^3.3.2"
  }
}
